    <blockTable colWidths="80.0,30.0,200.0,45.0,45.0,60.0" style="ITEMSTABLE">
          <tr>
            <td><para style="ITEMSRIGHT">[[line.product_id.name]]</para></td>
            <td><para style="ITEMSRIGHT">[[formatLang(line.product_uom_qty, digits=0)]]</para></td>
            <td><para style="ITEMSLEFTBLACK">[[line.special_ship]]</para></td>
            <td><para style="ITEMSRIGHT">[[line.ship_invoiced or 'No']]</para></td>
            <td><para style="ITEMSRIGHT">[[line.partial_supply or 'No']]</para></td>
            <td><para style="ITEMSRIGHT">[[line.date_arranged]]</para></td>
          </tr>
    </blockTable>
  </section>
  </pto>
  </story>
</document>
