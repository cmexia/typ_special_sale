</section>
<section>
      <blockTable colWidths="460" style="NOTETABLEHEADER">
        <tr>
          <td><para style="NOTESHEADER">Observaciones Generales</para></td>
        </tr>
        <tr>
          <td><para style="ITEMSLEFTBLACK">[[o.note]]</para></td>
      </tr>
      </blockTable>
    </section>

    <section>
      <blockTable colWidths="460" style="SPECIALSHIPTABLE">
        <tr>
          <td><para style="SPECIALSHIPHEADER">Embarque Especial</para></td>
        </tr>
        <tr>
          <td><para style="ITEMSLEFTBLACK">[[o.special_ship_global]]</para></td>
      </tr>
      </blockTable>
    </section>
<section>
<blockTable colWidths="80.0,30.0,200.0,45.0,45.0,60.0" style="ITEMSTABLEHEADER">
    <tr>
      <td><para style="ITEMSLEFT">Modelo</para></td>
      <td><para style="ITEMSLEFT">Ctdad</para></td>
